<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">torinomotors_moduleajax_distribuidoras_listing.torinomotors_moduleajax_distribuidoras_listing_data_source</item>
            <item name="deps" xsi:type="string">torinomotors_moduleajax_distribuidoras_listing.torinomotors_moduleajax_distribuidoras_listing_data_source</item>
        </item>
        <item name="spinner" xsi:type="string">spinner_columns</item>
    </argument>
    <dataSource name="torinomotors_moduleajax_distribuidoras_listing_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider</argument>
            <argument name="name" xsi:type="string">torinomotors_moduleajax_distribuidoras_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">dealer_place_id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="provider" xsi:type="string">torinomotors_moduleajax_marcas_listing.torinomotors_moduleajax_marcas_listing_data_source</item>
                    <item name="component" xsi:type="string">TorinoMotors_ModuleAjax/js/grid/provider/distribuidoras</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render" />
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">dealer_place_id</item>
                    </item>
                    <item name="imports" xsi:type="array">
                        <item name="params.recordId" xsi:type="string">torinomotors_moduleajax_marcas_listing.torinomotors_moduleajax_marcas_listing.modal_distribuidoras:recordId</item>
                        <item name="rootSelector" xsi:type="string">torinomotors_moduleajax_marcas_listing.torinomotors_moduleajax_marcas_listing.modal_distribuidoras:rootSelector</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>
    <listingToolbar name="listing_top">
        <bookmark name="bookmarks" />
        <columnsControls name="columns_controls" />
        <filters name="listing_filters">
            <filterSelect name="store_id" provider="${ $.parentName }">
                <settings>
                    <options class="Magento\Store\Ui\Component\Listing\Column\Store\Options" />
                    <caption translate="true">All Store</caption>
                    <label translate="true">Store View</label>
                    <dataScope>store_id</dataScope>
                </settings>
            </filterSelect>
        </filters>
        <paging name="listing_paging" />
        <massaction name="listing_massaction">
            <argument name="data" xsi:type="array">
                <item name="data" xsi:type="array">
                    <item name="selectProvider" xsi:type="string">torinomotors_moduleajax_distribuidoras_listing.torinomotors_moduleajax_distribuidoras_listing.spinner_columns.ids</item>
                    <item name="displayArea" xsi:type="string">bottom</item>
                    <item name="indexField" xsi:type="string">dealer_place_id</item>
                </item>
            </argument>
            <action name="delete">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">delete</item>
                        <item name="label" xsi:type="string" translate="true">Delete</item>
                        <item name="url" xsi:type="url" path="ajaxrequestadmin/marcas/massdelete" />
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Delete Marca</item>
                            <item name="message" xsi:type="string" translate="true">Eliminar Marca</item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
    <columns name="spinner_columns">
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="indexField" xsi:type="string">dealer_place_id</item>
                </item>
            </argument>
        </selectionsColumn>
        <column name="dealer_place_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">Dealer_Place_ID</item>
                </item>
            </argument>
        </column>
        <column name="marca_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">Marca_Id</item>
                </item>
            </argument>
        </column>
        <column name="dealer_place">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">Dealer_Place</item>
                </item>
            </argument>
        </column>
        <column name="created_at">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">FechaCreaci√≥n</item>
                </item>
            </argument>
        </column>
        <actionsColumn name="actions" class="TorinoMotors\ModuleAjax\Ui\Component\Listing\Column\DistribuidorasActions">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="string">107</item>
                    <item name="indexField" xsi:type="string">dealer_place_id</item>
                    <item name="sortOrder" xsi:type="number">180</item>
                </item>
            </argument>
        </actionsColumn>
    </columns>
    <modal name="modal_distribuidoras_form" component="TorinoMotors_ModuleAjax/js/modal/modal-component/distribuidoras" >
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="onCancel" xsi:type="string">actionCancel</item>
                <item name="options" xsi:type="array">
                    <item name="buttons" xsi:type="array">
                        <item name="0" xsi:type="array">
                            <item name="text" xsi:type="string">Close</item>
                            <item name="class" xsi:type="string">action-secondary</item>
                            <item name="actions" xsi:type="array">
                                <item name="0" xsi:type="string">actionCancel</item>
                            </item>
                        </item>
                        <item name="1" xsi:type="array">
                            <item name="text" xsi:type="string">Refresh</item>
                            <item name="class" xsi:type="string">action-secondary</item>
                            <item name="actions" xsi:type="array">
                                <item name="0" xsi:type="string">forceReload</item>
                            </item>
                        </item>
                        <!-- <item name="2" xsi:type="array">
                            <item name="text" xsi:type="string">Add</item>
                            <item name="class" xsi:type="string">action-primary</item>
                            <item name="actions" xsi:type="array">
                                <item name="0" xsi:type="array">
                                    <item name="targetName" xsi:type="string">oficina_refiere_notas_listing.oficina_refiere_notas_listing.modal_create_nota</item>
                                    <item name="actionName" xsi:type="string">openModal</item>
                                </item>
                            </item>
                        </item> -->
                    </item>
                    <item name="title" xsi:type="string">Editar Distribuidora</item>
                </item>
            </item>
        </argument>
        <insertForm name="torinomotors_form_distribuidoras">
        <settings>
            <dataLinks>
                <exports>false</exports>
                <imports>true</imports>
            </dataLinks>
            <formSubmitType>ajax</formSubmitType>
            <renderUrl path="mui/index/render">
                <param name="handle">torinomotors_form_distribuidoras</param>
                <param name="buttons">0</param>
                <param name="dealer_place_id">3</param>
            </renderUrl>
            <loading>true</loading>
            <autoRender>true</autoRender>
            <toolbarContainer>${ $.parentName }</toolbarContainer>
            <ns>torinomotors_form_distribuidoras</ns>
        </settings>
    </insertForm>
    </modal>
</listing>
