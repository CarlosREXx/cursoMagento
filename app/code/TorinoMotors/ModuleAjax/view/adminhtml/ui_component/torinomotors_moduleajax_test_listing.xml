    <listingToolbar name="listing_top">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="sticky" xsi:type="boolean">true</item>
            </item>
        </argument>

        <filters name="listing_filters" displayArea="dataGridFilters">

            <settings>
                <dataScope>filters</dataScope>
                <childDefaults>
                    <param name="imports" xsi:type="array">
                        <item name="visible" xsi:type="string">torinomotors_moduleajax_test_listing.torinomotors_moduleajax_test_listing.listing_top.bookmarks:current.columns.${ $.index }.visible</item>
                    </param>
                    <param name="provider" xsi:type="string">torinomotors_moduleajax_test_listing.torinomotors_moduleajax_test_listing.listing_top.listing_filters</param>
                </childDefaults>
                <storageConfig>
                    <namespace>current.filters</namespace>
                    <provider>torinomotors_moduleajax_test_listing.torinomotors_moduleajax_test_listing.listing_top.bookmarks</provider>
                </storageConfig>
            </settings>

            <filterInput name="id" provider="${ $.parentName }">
                <settings>
                    <label translate="true">Id</label>
                    <dataScope>id</dataScope>
                </settings>
            </filterInput>

        </filters>

        <paging name="listing_paging">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="selectProvider" xsi:type="string">torinomotors_moduleajax_test_listing.torinomotors_moduleajax_test_listing.spinner_columns.ids</item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">torinomotors_moduleajax_test_listing.torinomotors_moduleajax_test_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.paging</item>
                    </item>
                </item>
            </argument>
        </paging>

    </listingToolbar>
    <columns name="spinner_columns">
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="indexField" xsi:type="string">id</item>
                </item>
            </argument>
        </selectionsColumn>
        <column name="id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <!-- <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item> -->
                    <item name="label" xsi:type="string" translate="true">ID</item>
                </item>
            </argument>
        </column>
        <column name="name">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <!-- <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item> -->
                    <item name="label" xsi:type="string" translate="true">Nombre</item>
                </item>
            </argument>
        </column>
        <column name="genero">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <!-- <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item> -->
                    <item name="label" xsi:type="string" translate="true">Genero</item>
                </item>
            </argument>
        </column>
        <!-- <actionsColumn name="actions" class="TorinoMotors\ModuleAjax\Ui\Component\Listing\Column\DistribuidorasActions">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
